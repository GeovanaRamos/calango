      &lt;font face="Courier New"&gt;&lt;span class="c0"&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;escreval(numero);&lt;/span&gt;&lt;/font&gt;
    &lt;/p&gt;
    &lt;p style="white-space: normal; margin-top: 0px; text-transform: none; font-variant: normal; margin-right: 0px; text-indent: 119.2pt; margin-left: 0px; word-spacing: 0px; line-height: normal; margin-bottom: 0px; letter-spacing: normal" class="c9 c26" align="start"&gt;
      &lt;font face="Courier New"&gt;&lt;span class="c0"&gt;fimPrincipal&lt;/span&gt;&lt;/font&gt;
